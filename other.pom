        <profile>
            <id>qftest</id>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.codehaus.groovy.maven</groupId>
                        <artifactId>gmaven-plugin</artifactId>
                        <executions>
                            <execution>
                                <phase>install</phase>
                                <goals>
                                    <goal>execute</goal>
                                </goals>
                                <configuration>
                                    <verbose>true</verbose>
                                    <properties>
                                        <qftestexe>${qftest.exe}</qftestexe>
                                        <qftestmaxheapsize>${qftest.maxheapsize}</qftestmaxheapsize>
                                        <qftestsrc>${qftest.src}</qftestsrc>
                                    </properties>
                                    <source>
                                        println "Preparing environment for QF-Test ..."
                                        // directory defs
                                        def buildDir = "${project.build.directory}"
                                        def branding = project.properties["branding-token"]
                                        def userDataDir = "${buildDir}/userdata"

                                        // create serverdata dir, if not existant
                                        new File("${userDataDir}").mkdir()
                                        //make sure no failed security test is left a security realm configured:
                                        new File("${buildDir}/${branding}/security.properties").delete();

                                        // Patch app.conf with userdata dir and maxheapsize
                                        def myFile = new File("${buildDir}/gridvis/etc/gridvis.conf")
                                        def fileText = myFile.text
                                        fileText = fileText.replace('${HOME}', "${userDataDir}")
                                        fileText = fileText.replace('${APPNAME}', "gridvis")
                                        fileText = fileText.replace('${installer:maxHeapSize}', "${project.properties.qftestmaxheapsize}")
                                        myFile.write(fileText)

                                        def LOGS = "${buildDir}/surefire-reports"

                                        println "Starting QF-Test ..."

                                        def cmd = """${project.properties.qftestexe} -nomessagewindow -J-Xmx1024m -exitcodeignoreexception -batch -compact -runlog $LOGS/TEST-QF-Desktop -report.html $LOGS -report.junit $LOGS -variable errorReportDumpDir=${LOGS} -variable pathGridvis=${buildDir}/${branding} -variable versionGridvis=${project.version} ${project.properties.qftestsrc}/gridvis-qftests/TestSets/setQuick.qft"""
                                        println "&gt;&gt;&gt; " + cmd
                                        Process pQF = cmd.execute()
                                        pQF.waitForProcessOutput System.out,System.err
                                        //quick test should be finished in 5minutes
                                        pQF.waitForOrKill(5*60*1000)


                                        println "return code: ${ pQF.exitValue()}"

                                        println "QF-Test finished ..."
                                    </source>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
