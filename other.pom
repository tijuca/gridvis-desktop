        <app-name>Janitza-GridVis</app-name>
        <app-title>GridVis</app-title>
        <app-version-number>7.1</app-version-number>
        <app-version>${project.version}</app-version>
        <app-publisher>Janitza electronics GmbH</app-publisher>
        <app-publisher-url>www.janitza.de</app-publisher-url>
        <app-installerid>5192-2920-3443-4425</app-installerid>
        <app-PBColor>0x6FA4D8</app-PBColor>
        <app-PBBounds>0,273,380,6</app-PBBounds>
        <app-PTColor>0x000000</app-PTColor>
        <app-PTBounds>6,285,380,10</app-PTBounds>
        <branding-token>gridvis</branding-token>
        <brandingToken>gridvis</brandingToken>
        <app-splashWidth>400</app-splashWidth>
        <app-startmenu>GridVis</app-startmenu>
        <licsense-url>http://license.janitza.de</licsense-url>
        <requestfile-extension>gridvisreq</requestfile-extension>
        <licensefile-extension>gridvislic</licensefile-extension>

        <netbeans.version.unit>${netbeans.version}</netbeans.version.unit>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <!-- Tell the cluster plugin to use our app.conf. basedir is this one. -->
        <netbeans.conf.file>app.conf</netbeans.conf.file>
        <jenkins-creator-pin>gridvis-docker</jenkins-creator-pin>
        <jenkins-creator-package>app</jenkins-creator-package>
        <!-- Make sure we have the latest cluster versions. -->
        <sanity-force-cluster-version-on>.*</sanity-force-cluster-version-on>

        <build.temp>${java.io.tmpdir}/${project.version}</build.temp>
        <netbeans.run.params.ide/>
        <!-- The run parameters are only for the IDE!. To change the parameters use the applications/*/app.conf file. -->

        <!-- activate own logback config via profile  -->
        <cli-own-logback-config/>
        <!-- configure your feature toggles in .m2/settings.xml-->
        <cli-own-features-from-m2/>
        <cli-logback>-J-Dnetbeans.mainclass=de.janitza.pasw.arg.catcher.MainClass -J-Djava.util.logging.config.class=de.janitza.pasw.pasw.logback.JulHandler ${cli-own-logback-config}</cli-logback>
        <cli-jvm> -J-Xms64m -J-Xmx500m -J-Dvisualvm.display.name=${branding-token}</cli-jvm>
        <cli-netbeans>--branding ${branding-token} --locale de --laf com.sun.java.swing.plaf.windows.WindowsLookAndFeel</cli-netbeans>
        <cli-gridvis>-J-Dfast-status-mail=true  -J-Dsharedplugin-live-reload=false -J-DJAN_DEBUG_DISABLE_RELOAD=false -J-Dalarm-emulate-server=true -J-Dalarm-emulate-time=true </cli-gridvis>
        <netbeans.run.params>${netbeans.run.params.ide}  ${cli-logback} ${cli-jvm} ${cli-netbeans} ${cli-gridvis} ${cli-own-features-from-m2}</netbeans.run.params>
        <reportsBaseDir>${project.build.directory}/${branding-token}/reports</reportsBaseDir>
    </properties>

